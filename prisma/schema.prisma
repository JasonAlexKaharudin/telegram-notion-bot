generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model food_logger {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  date     DateTime @db.Date
  foodName String
  mealType String
  user     String

  @@map("food-logger")
}
